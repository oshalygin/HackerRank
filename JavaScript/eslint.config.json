{
	"extends": "eslint:recommended",
	"rules": {
		"no-bitwise": 0,
		"camelcase": 1,
		"curly": 1,
		"eqeqeq": 1,
		"no-eq-null": 1,
		"guard-for-in": 1,
        "no-eval": 1,
        "no-fallthrough": 1,
        "no-implied-eval": 1,
        "no-proto": 1,
        "vars-on-top": 1,
        "no-with": 1,
        ""
		"no-empty": 1,
        "no-console": 0
	},
	"globals": {
		"$": false
	},
    "ecmafeatures": {
        "modules": true
    },
	"env":{
		"browser": true,
		"node": true,
        "es6": true
	}
}
